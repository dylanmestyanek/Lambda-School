    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "ci": "^1.0.0",
    "compression": "1.7.0",
    "concurrently": "3.5.0",
    "cypress": "^3.4.1",
    "express": "4.15.4",
    "express-http-proxy": "1.0.6",
    "lodash": "4.17.13",
